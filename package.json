{
  "name": "finance-whatsapp-bot",
  "version": "1.0.0",
  "description": "Bot de WhatsApp integrado com Google Sheets para controle financeiro",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:whatsapp": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:whatsapp": "node dist/index.js",
    "clean": "rm -rf dist",
    "test:sheets": "ts-node src/tests/test-sheets.ts",
    "test:ai": "ts-node src/tests/test-ai.ts"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "baileys",
    "google-sheets",
    "finance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@google/generative-ai": "^0.24.1",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "googleapis": "^140.0.1",
    "link-preview-js": "^3.2.0",
    "pino": "^9.0.0",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/qrcode-terminal": "^0.12.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
